            VALUE "ProductVersion", "@PROJECT_VERSION_MAJOR@.@PROJECT_VERSION_MINOR@.@PROJECT_VERSION_PATCH@.@GIT_COMMIT_HASH@"
        END
    END
    BLOCK "VarFileInfo"
